.type-list-soft-item > li > .title {
  height: 40px;
  line-height: 40px;
  color: #33cccc;
  background-color: #f2f2f2;
  padding-left: 20px;
}
.type-list-soft-item > li > .content {
  position: relative;
  margin-left: 30px;
  padding-top: 10px;
}
.type-list-soft-item > li > .content > div {
  float: left;
  width: 33%;
  line-height: 1;
  font-weight: 700;
  margin-bottom: 10px;
}
.type-list-soft-item > li > .content > div > span {
  font-weight: 400;
}
.type-list-soft-item > li > .content > .w-max {
  width: 100%;
}
.type-list-soft-item > li > .content > .soft-after > span {
  color: red;
}
.type-list-soft-item > li > .content > .register-date {
  width: auto;
  margin: 0;
  padding: 0;
  position: absolute;
  bottom: 10px;
  right: 20px;
}
