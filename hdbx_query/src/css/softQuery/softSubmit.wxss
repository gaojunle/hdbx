.soft-submit > .w1000 {
  width: 1000px;
  margin: 0 auto;
}
.soft-submit > .w1000 > .progressNav {
  margin-bottom: 38px;
}
.soft-submit > .w1000 > .buttons {
  width: 500px;
  margin: 50px auto 0;
}
.soft-submit > .w1000 > .buttons > div {
  width: 140px;
  height: 40px;
  border-radius: 6px;
  cursor: pointer;
  text-align: center;
  line-height: 40px;
  box-sizing: border-box;
}
.soft-submit > .w1000 > .buttons > .prev {
  float: left;
  border: 1px solid #c7c7c7;
}
.soft-submit > .w1000 > .buttons > .next {
  float: right;
  color: #fff;
  background-color: #169bd5;
}
