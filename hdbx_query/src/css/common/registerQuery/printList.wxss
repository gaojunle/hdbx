.print-list {
  margin-bottom: 50px;
}
.print-list > .title {
  padding-left: 60px;
  height: 50px;
  line-height: 50px;
  background-color: #f2f2f2;
  font-size: 16px;
}
.print-list > ul > li {
  height: 40px;
  line-height: 40px;
}
.print-list > ul > li > div {
  float: left;
}
.print-list > ul > li > .num {
  width: 80px;
  margin-left: 20px;
}
.print-list > ul > li > .name {
  width: 400px;
}
.print-list > ul > li > .requirement {
  width: 300px;
}
.print-list > ul > li > .operation {
  cursor: pointer;
}
.print-list > ul > li > .print {
  margin-left: 60px;
  cursor: pointer;
}
.print-list > ul > .list-title {
  border-bottom: 1px solid #f2f2f2;
  height: 60px;
  line-height: 60px;
}
.print-list > ul > .list-title > .operation {
  cursor: auto;
}
.print-list > ul > .list-title > .print {
  width: 100px;
  height: 40px;
  margin-top: 10px;
  margin-left: 50px;
  background-color: #169bd5;
  border-radius: 6px;
  text-align: center;
  line-height: 40px;
  color: #fff;
}
