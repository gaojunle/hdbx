.progressNav > .query-crumbs {
  text-align: center;
}
.progressNav > .query-crumbs > ul > li > div .crumbs-title {
  position: relative;
  line-height: 60px;
}
.progressNav > .query-crumbs > ul > li > div > .crumbs-title > .crumbs-img {
  line-height: none;
  position: absolute;
  left: -30px;
  top: -12px;
}
.progressNav > .query-crumbs ul {
  list-style: none;
  display: inline-table;
}
.progressNav > .query-crumbs ul li {
  display: inline;
}
.progressNav > .query-crumbs ul li div {
  display: block;
  float: left;
  height: 50px;
  background: #ededed;
  text-align: center;
  width: 386px;
  height: 60px;
  position: relative;
  margin: 0 10px 0 0;
  font-size: 18px;
  text-decoration: none;
  color: #333333;
}
.progressNav > .query-crumbs ul .elastic div {
  display: block;
  float: left;
  height: 50px;
  background: #ededed;
  text-align: center;
  width: 320px !important;
  height: 60px;
  position: relative;
  margin: 0 10px 0 0;
  font-size: 18px;
  text-decoration: none;
  color: #333333;
}
.progressNav > .query-crumbs ul li div:after {
  content: "";
  border-top: 30px solid transparent;
  border-bottom: 30px solid transparent;
  border-left: 30px solid #ededed;
  position: absolute;
  right: -30px;
  top: 0;
  z-index: 1;
}
.progressNav > .query-crumbs ul li div:before {
  content: "";
  border-top: 30px solid transparent;
  border-bottom: 30px solid transparent;
  border-left: 30px solid #fff;
  position: absolute;
  left: 0;
  top: 0;
}
.progressNav > .query-crumbs ul li:first-child div {
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
}
.progressNav > .query-crumbs ul li:last-child div {
  margin: 0;
}
.progressNav > .query-crumbs ul li:first-child a:before {
  display: none;
}
.progressNav > .query-crumbs ul li:last-child div {
  padding-right: 20px;
  border-top-right-radius: 10px;
  border-bottom-right-radius: 10px;
}
.progressNav > .query-crumbs ul li:last-child div:after {
  display: none;
}
.progressNav > .query-crumbs ul .crumbs div {
  background: #005db8;
  color: #fff;
}
.progressNav > .query-crumbs ul .crumbs div:after {
  border-left-color: #005db8;
}
