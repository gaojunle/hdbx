.box {
  width: 1000px;
  height: 1335px;
  margin: 0 auto;
  background: #fff;
  font-size: 18px;
  font-family: SimSun;
  position: relative;
}
.box header {
  width: 100%;
  height: 120px;
  background: #f2f2f2;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 10px 0;
  box-sizing: border-box;
}
.box header .left {
  margin-left: 40px;
}
.box header .right {
  width: 100px;
  height: 100px;
}
.box header .right img {
  width: 100%;
  height: 100%;
}
.box .h1 {
  text-align: center;
  font-weight: bold;
  font-size: 26px;
}
.box .formLeft {
  padding-left: 54px;
  line-height: 36px;
  position: relative;
}
.box .rowRight {
  border-bottom: 1px solid #333;
  line-height: initial;
  position: absolute;
  bottom: 4px;
  width: 80%;
  display: inline-block;
}
.box .one {
  padding-left: 54px;
  font-size: 18px;
  font-family: SimSun;
}
.box .one .text1 {
  margin-bottom: 20px;
  font-size: 15px;
  margin-top: 30px;
}
.box .one .list {
  margin-bottom: 120px;
}
.box .bao {
  position: absolute;
  right: 50px;
  bottom: 200px;
}
.box .bao p {
  line-height: 30px;
}
.box .footer {
  width: 896px;
  padding: 0 52px;
  position: absolute;
  bottom: 82px;
  left: 0;
  height: 82px;
}
.box .footer .line {
  border-bottom: 3px solid #666;
}
.box .footer > .footerPhone {
  font-size: 14px;
  margin-bottom: 22px;
  font-family: SimSun;
}
.box .footer > .footerPhone > div {
  width: 33%;
  float: left;
}
.box .footer > .footerAddress {
  margin-top: 18px;
  font-size: 16px;
  font-family: SimSun;
}
.box .footer > .footerAddress > span {
  margin-left: 35px;
}
.indentation {
  padding-left: 30px;
}
