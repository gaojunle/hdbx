.box {
  width: 800px;
  height: 1335px;
  margin: 0 auto;
  background: #fff;
  position: relative;
}
.box header {
  width: 100%;
  height: 120px;
  background: #f2f2f2;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 10px 0;
  box-sizing: border-box;
}
.box header .left {
  margin-left: 40px;
}
.box header .right {
  width: 180px;
  height: 180px;
}
.box header .right .p1 {
  font-weight: bold;
  color: #c00;
}
.box header .right .p2 {
  font-weight: bold;
  font-size: 14px;
}
.box .h1 {
  text-align: center;
  font-weight: bold;
  font-size: 26px;
}
.box .formLeft {
  padding-left: 54px;
  line-height: 36px;
  position: relative;
}
.box .rowRight {
  border-bottom: 1px solid #333;
  line-height: initial;
  position: absolute;
  bottom: 4px;
  width: 80%;
  display: inline-block;
}
.box .one {
  padding-left: 20px;
  padding-top: 10px;
}
.box .one .text1 {
  margin-bottom: 20px;
  margin-top: 30px;
}
.box .one .hr span {
  border-bottom: 1px solid #000;
}
.box .one .list {
  margin-bottom: 20px;
  margin-top: 20px;
}
.box .one .list .p {
  position: relative;
}
.box .one .list .p .kou {
  position: absolute;
  left: 10px;
  top: 3px;
  width: 15px;
  height: 15px;
  border: 1px solid #000;
  text-align: center;
  line-height: 15px;
}
.box .pp p {
  line-height: 35px;
}
.box .bao {
  position: absolute;
  right: 50px;
  bottom: 300px;
}
.box .bao p {
  line-height: 30px;
}
.box .bao .p {
  float: right;
}
.box .footer {
  width: 800px;
  position: absolute;
  bottom: 140px;
  left: -30px;
  height: 82px;
}
.box .footer .zu {
  font-size: 18px;
  margin-bottom: 10px;
}
.box .footer .line {
  border-bottom: 3px solid #666;
}
.box .footer > .footerPhone {
  font-size: 14px;
  margin-bottom: 22px;
}
.box .footer > .footerPhone > div {
  width: 33%;
  float: left;
}
.box .footer > .footerAddress {
  margin-top: 18px;
}
.box .footer > .footerAddress > span {
  margin-left: 35px;
}
.indentation {
  padding-left: 30px;
}
